{
    "line-end-style": "lf",
    "line-max-len": 240,
    "indent-style": "spaces",
    "indent-width": 2,
    "tag-close": true,
    "tag-self-close": "never",
    "tag-name-match": true,
    "tag-name-lowercase": true,
    "tag-bans": [
        "style",
        "b",
        "i"
    ],
    "doctype-first": false,
    "doctype-html5": true,
    "html-valid-content-model": true,
    "html-req-lang": true,
    "lang-style": "xx-YY",
    "head-req-title": true,
    "head-valid-content-model": true,
    "attr-name-style": "dash",
    "attr-no-dup": true,
    "attr-quote-style": "double",
    "attr-order": [
        "id",
        "class",
        "rel",
        "type",
        "src",
        "href",
        "alt"
    ],
    "attr-req-value": false,
    "attr-new-line": 5,
    "attr-bans": [
        "align",
        "background",
        "bgcolor",
        "border",
        "frameborder",
        "longdesc",
        "marginwidth",
        "marginheight",
        "scrolling",
        "style",
        "width",
        "height"
    ],
    "href-style": "relative",
    "id-class-style": "dash",
    "id-no-dup": true,
    "class-no-dup": false,
    "class-style": "dash",
    "img-req-alt": true,
    "img-req-src": true,
    "title-no-dup": true,
    "title-max-len": 60,
    "spec-char-escape": false
}
